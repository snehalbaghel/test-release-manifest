{
    "manifest_version": 2,
    "name": "OSlash",
    "version": "3.2.12",
    "description": "Boosts your productivity with keyboard driven actions.",
    "icons": {
        "48": "assets/icons/icon48.png",
        "128": "assets/icons/icon128.png"
    },
    "web_accessible_resources": [
        "launcher/launcher.html",
        "oauth/index.html",
        "pinInstaller.css",
        "pinInstaller.js",
        "dashboard/*"
    ],
    "background": {
        "page": "background/index.html",
        "persistent": true
    },
    "commands": {
        "search_bar": {
            "description": "Launches OSlash search",
            "suggested_key": {
                "chromeos": "Ctrl+J",
                "linux": "Alt+J",
                "mac": "Command+J",
                "windows": "Alt+J"
            }
        }
    },
    "options_ui": {
        "page": "dashboard/login.html",
        "open_in_tab": true
    },
    "permissions": [
        "tabs",
        "windows",
        "history",
        "topSites",
        "<all_urls>",
        "storage",
        "unlimitedStorage",
        "activeTab",
        "webRequest",
        "webRequestBlocking",
        "*://o/*",
        "*://m/*",
        "*://*.google.com/",
        "background",
        "cookies",
        "identity"
    ],
    "externally_connectable": {
        "matches": [
            "*://app.oslash.com/*",
            "*://oslash.com/*",
            "*://sentry.corp.oslash.com/*"
        ]
    },
    "omnibox": {
        "keyword": "o/"
    },
    "oauth2": {
        "client_id": "798795782174-e05pnh0iuado0pu8a3ul3a18be5i422j.apps.googleusercontent.com",
        "scopes": [
            ""
        ]
    },
    "browser_action": {
        "default_title": "OSlash"
    },
    "key": ""
}
